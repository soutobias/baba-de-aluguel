<div class="navbar navbar-expand-sm navbar-light navbar-lewagon fixed-top mt-0">
  <%= link_to root_path, class: "navbar-brand" do %>
  <%= image_tag "data:image/png;base64," %>
    <% end %>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse content" id="navbarSupportedContent content">
    <ul class="navbar-nav mr-auto">
      <% if user_signed_in? %>
        <li class="nav-item">
          <%= link_to "Messages", "#", class: "nav-link login-btn" %>
        </li>
        <li class="nav-item dropdown">
          <%= image_tag "https://avatars2.githubusercontent.com/u/39573110?s=400&u=5eb905f84111c8de21a760f725fa6d09d8fba569&v=4", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
          <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
            <%= link_to "Action", "#", class: "dropdown-item" %>
            <%= link_to "Another action", "#", class: "dropdown-item" %>
            <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
          </div>
        </li>
      <% else %>
        <li class="nav-item">
          <%= link_to "Login", new_user_session_path, class: "nav-link login-btn" %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
